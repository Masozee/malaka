<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>com.malaka.traefik</string>

    <key>ProgramArguments</key>
    <array>
        <string>/opt/homebrew/bin/traefik</string>
        <string>--configfile=/Users/pro/Dev/malaka/traefik/traefik-dev.yml</string>
    </array>

    <key>WorkingDirectory</key>
    <string>/Users/pro/Dev/malaka/traefik</string>

    <key>RunAtLoad</key>
    <true/>

    <key>KeepAlive</key>
    <dict>
        <key>SuccessfulExit</key>
        <false/>
    </dict>

    <key>StandardOutPath</key>
    <string>/Users/pro/Dev/malaka/logs/traefik/stdout.log</string>

    <key>StandardErrorPath</key>
    <string>/Users/pro/Dev/malaka/logs/traefik/stderr.log</string>

    <key>ThrottleInterval</key>
    <integer>10</integer>
</dict>
</plist>
